$newTableProperties = [ordered]@{
    qInfo = [ordered]@{
        qType = "table"
        qId = $DashboardSheet.Replace(' ', '')
    }
    qHyperCubeDef = [ordered]@{
        qDimensions = @()
        qMeasures = @()
