foreach ($dimension in $dimensions) {
    $qDimension = @{
        qDef = @{
            qFieldDefs = $dimension.qFieldDefs
            # add any other properties not defined in $dimension
            $dimension.PSObject.Properties | Where-Object { $_.Name -ne "qFieldDefs" } | ForEach-Object {
                if (-not $qDimension.qDef.ContainsKey($_.Name)) {
                    $qDimension.qDef.Add($_.Name, $_.Value)
                }
            }
        }
    }
    $newTableProperties.qHyperCubeDef.qDimensions += $qDimension
}
